{
  "name": "myng",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ionic-app-scripts build --aotWriteToDisk",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "bundle": "ionic-app-scripts bundle",
    "minify": "ionic-app-scripts minify --generateSourceMap=false",
    "prod": "ionic-app-scripts build --prod --aot --minifyjs --minifycss --optimizejs --wwwDir dist",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "dexie": "^2.0.4",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "qiniu-js": "^2.5.1",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^3.2.1",
    "typescript": "~2.4.2"
  }
